{
	"name": "mhr-mod-manager",
	"version": "1.0.0",
	"description": "Monster Hunter Rise Mod Manager for FirstNative",
	"main": "./dist/main.js",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"lint": "eslint -c .eslintrc --ext .ts ./src",
		"dev": "npm run build && electron ./dist/main.js",
		"pack": "rm -r target && electron-packager . MHR-Mod-Manager --prune --ignore=\\.ts$ --out=target --platform=win32 --arch=x64 && cp src/config.default.yml target/MHR-Mod-Manager-win32-x64/config.yml"
	},
	"repository": "https://github.com/electron/electron-quick-start-typescript",
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo",
		"typescript"
	],
	"devDependencies": {
		"@types/feather-icons": "^4.7.0",
		"@types/js-yaml": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"electron": "^19.0.9",
		"electron-packager": "^15.5.1",
		"electron-reload": "^2.0.0-alpha.1",
		"eslint": "^8.20.0",
		"eslint-plugin-sonarjs": "^0.14.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"feather-icons": "^4.29.0",
		"js-yaml": "^4.1.0",
		"node-unrar-js": "^2.0.0"
	}
}
